{
	"name": "modern-saas",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"gen:types": "npx supabase gen types typescript --local > src/lib/supabase-types.ts && prettier --write src/lib/supabase-types.ts",
		"seed": "tsx --tsconfig scripts/tsconfig.json scripts/seed.ts"
	},
	"devDependencies": {
		"@faker-js/faker": "^8.1.0",
		"@playwright/test": "^1.38.1",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/adapter-vercel": "^3.0.3",
		"@sveltejs/kit": "^1.25.0",
		"@types/detect-port": "^1.3.3",
		"@types/pg": "^8.10.3",
		"@typescript-eslint/eslint-plugin": "^6.7.2",
		"@typescript-eslint/parser": "^6.7.2",
		"autoprefixer": "^10.4.16",
		"detect-port": "^1.5.1",
		"eslint": "^8.50.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.33.2",
		"pg": "^8.11.3",
		"postcss": "^8.4.30",
		"postcss-load-config": "^4.0.1",
		"prettier": "^3.0.3",
		"prettier-plugin-svelte": "^3.0.3",
		"stripe-event-types": "^3.0.0",
		"supabase": "^1.99.5",
		"svelte": "^4.2.1",
		"svelte-check": "^3.5.2",
		"svelte-preprocess": "^5.0.4",
		"sveltekit-superforms": "^1.7.1",
		"tailwindcss": "^3.3.3",
		"tslib": "^2.6.2",
		"tsx": "^3.12.10",
		"typescript": "^5.2.2",
		"vite": "^4.4.9",
		"vitest": "^0.34.5",
		"zod": "^3.22.2"
	},
	"type": "module",
	"dependencies": {
		"@popperjs/core": "^2.11.8",
		"@supabase/auth-helpers-sveltekit": "^0.10.3",
		"@supabase/supabase-js": "^2.36.0",
		"classnames": "^2.3.2",
		"dotenv": "^16.3.1",
		"flowbite": "^1.8.1",
		"flowbite-svelte": "^0.44.17",
		"stripe": "^13.7.0",
		"svelte-french-toast": "1.2.0"
	}
}
